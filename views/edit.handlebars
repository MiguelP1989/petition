<form action="/profile/edit" method="POST">
    <h1>Profile Edit</h1>

<input  type="text" name="first" value="{{first}}" placeholder="First name..">
<!-- <input type="hidden" name="_csrf" value="{{csrfToken}}"> -->
<input  type="text" name="last" value="{{last}}" placeholder="Last name..">
<input  type="text" name="age" value="{{age}}" placeholder="Age">
<input  type="text" name="city" value="{{city}}" placeholder="City">
<input type="text" name="email" value="{{email}}" placeholder="Email">
<input type="password" name="password" placeholder="Password..">
<input type="text" name="url" placeholder="Homepage">

<div class ="button">
    <button type="submit">Update
    </button>
</div>
</form>
